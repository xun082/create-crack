{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "test": {
      "dependsOn": ["^test"]
    },
    "test:run": {
      "dependsOn": ["^test:run"]
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "with": ["@verve-kit/utils#dev"]
    }
  }
}
